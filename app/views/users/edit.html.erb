<div id="main_no_row">
  <p class="heading_4">Editing User Details</p>
  <%= render 'form' %>
  <% if current_user and current_user.admin %>
      <%= link_to 'Show', @user %> |
      <%= link_to 'Back', users_path %>
  <% else %>
      <%= link_to 'Back', :controller => "users", :action => "welcome", :notice => "Edited successfully" %>
  <% end %>
</div>